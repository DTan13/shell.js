{
  "name": "shell.js",
  "version": "2.0.9",
  "description": "A JavaScript and CSS library to create terminals",
  "scripts": {
    "dev": "NODE_ENV=dev webpack --watch",
    "build": "webpack-dev-server",
    "build:prod": "NODE_ENV=dev webpack; NODE_ENV=prod webpack -p",
    "test": "NODE_ENV=test mocha --timeout 15000 --require babel-core/register --require test/helpers/* test/*.test.js",
    "test:coverage": "nyc --reporter=lcov yarn test",
    "test:report": "yarn test:coverage; nyc report --reporter=html",
    "test:watch": "yarn test --watch"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true
  },
  "author": "Davide Caruso <davide.caruso93@gmail.com>",
  "license": "MIT",
  "keywords": [
    "shell",
    "shelljs",
    "javascript",
    "terminal-emulators",
    "terminal",
    "ecmascript2015",
    "command-line",
    "es2015",
    "javascript-library",
    "javascript-plugin"
  ],
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean": "^4.0.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "eslint": "^4.5.0",
    "expect": "^21.2.1",
    "istanbul": "^0.4.5",
    "jsdom": "^11.3.0",
    "jshint-stylish": "^2.2.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "should": "^13.1.2",
    "style-loader": "^0.18.2",
    "typed.js": "^2.0.6",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "universal-style-loader": "^0.16.3",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^2.11",
    "webpack-stream": "^4.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": "git+https://github.com/davidecaruso/shell.js.git",
  "bugs": {
    "url": "https://github.com/davidecaruso/shell.js/issues"
  },
  "homepage": "https://davidecaruso.github.io/shell.js",
  "main": "lib/shell.js"
}
