{
  "name": "shell.js",
  "version": "2.0.9",
  "description": "A JavaScript and CSS library to create terminals",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true
  },
  "author": "Davide Caruso <davide.caruso93@gmail.com>",
  "license": "MIT",
  "keywords": [
    "shell",
    "shelljs",
    "javascript",
    "terminal-emulators",
    "terminal",
    "ecmascript2015",
    "command-line",
    "es2015",
    "javascript-library",
    "javascript-plugin"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^7.1.4",
    "clean": "^4.0.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "eslint": "^4.5.0",
    "expect": "^21.2.1",
    "istanbul": "^0.4.5",
    "jsdom": "^11.3.0",
    "jshint-stylish": "^2.2.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "should": "^13.1.2",
    "style-loader": "^0.18.2",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "universal-style-loader": "^0.16.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.0",
    "webpack-merge": "^4.1.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": "git+https://github.com/davidecaruso/shell.js.git",
  "bugs": {
    "url": "https://github.com/davidecaruso/shell.js/issues"
  },
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js --mode=development --watch",
    "build": "webpack-dev-server --mode=development",
    "build:prod": "webpack --mode=development; webpack --mode=production -p",
    "test": "mocha --timeout 15000 --require babel-core/register --require test/helpers/* test/*.test.js",
    "test:coverage": "nyc --reporter=lcov npm run test",
    "test:report": "npm run test:coverage; nyc report --reporter=html",
    "test:watch": "npm run test --watch"
  },
  "homepage": "https://davidecaruso.github.io/shell.js",
  "main": "lib/shell.js"
}
